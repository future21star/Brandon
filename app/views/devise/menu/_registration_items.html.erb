<% if user_signed_in? %>
    <li>
      <%= link_to(current_user.email, edit_user_registration_path) %>
    </li>
<% else %>
    <li>
      <%= link_to('Register', new_user_registration_path)  %>
    </li>
    <li>
      <%= link_to('Business Sign-Up', new_user_registration_path({business: true})) %>
    </li>
<% end %>
